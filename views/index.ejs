<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <style>
		#map {
			width: 100%;
			height: 800px;
		}
	</style>
  </head>
  <body>
    <h1><%= title %></h1>
    <div id="map"></div>
    <script>
    	function initMap() {
    		var uluru = {lat: -25.363, lng: 131.044};
    		var map = new google.maps.Map(document.getElementById('map'), {
    			zoom: 4,
    			center: uluru
    		});
    		var marker = new google.maps.Marker({
    			position: uluru,
    			map: map
    		});
    	}
	</script>
    <script async defer
    	src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAJGZTs8mUHBXCm9EhqMf2LjUFlq9DcHx4&callback=initMap">
    </script>
  </body>
</html>